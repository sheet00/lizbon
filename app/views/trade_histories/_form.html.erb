	</div>
	<div class="field">
		<%= form.label :amount %>
		<%= form.text_field :amount, id: :trade_history_amount %>
	</div>
	<div class="field">
		<%= form.label :price %>
		<%= form.text_field :price, id: :trade_history_price %>
	</div>
	<div class="field">
		<%= form.label :fee %>
		<%= form.text_field :fee, id: :trade_history_fee %>
		<div class="field">
			<%= form.label :fee_amount %>
			<%= form.text_field :fee_amount, id: :trade_history_fee_amount %>
