<%= form_with(model: trade_history, local: true) do |form| %>
	<% if trade_history.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(trade_history.errors.count, "error") %> prohibited this trade_history from being saved:</h2>
			<ul>
				<% trade_history.errors.full_messages.each do |message| %>
					<li><%= message %></li>
				<% end %>
			</ul>
		</div>
	<% end %>
	<div class="field">
