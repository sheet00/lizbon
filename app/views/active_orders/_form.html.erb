		<%= form.text_field :amount, id: :active_order_amount %>
	</div>
	<div class="field">
		<%= form.label :price %>
		<%= form.text_field :price, id: :active_order_price %>
	</div>
	<div class="field">
		<%= form.label :timestamp %>
		<%= form.text_field :timestamp, id: :active_order_timestamp %>
	</div>
	<div class="field">
		<%= form.label :transaction_id %>
		<%= form.text_field :transaction_id, id: :active_order_transaction_id %>
	</div>
	<div class="field">
