<%= form_with(model: active_order, local: true) do |form| %>
	<% if active_order.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(active_order.errors.count, "error") %> prohibited this active_order from being saved:</h2>
			<ul>
				<% active_order.errors.full_messages.each do |message| %>
					<li><%= message %></li>
				<% end %>
			</ul>
		</div>
	<% end %>
	<div class="field">
		<%= form.label :order_id %>
		<%= form.number_field :order_id, id: :active_order_order_id %>
